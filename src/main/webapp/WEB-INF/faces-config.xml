<?xml version="1.0" encoding="windows-1252"?>
<faces-config>
    <navigation-rule>
        <from-view-id>login.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{loginBean.login()}</from-action>
            <from-outcome>login-success</from-outcome>
            <to-view-id>eventos.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
